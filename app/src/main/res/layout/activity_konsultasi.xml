<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".KonsultasiActivity">


    <View
        android:id="@+id/rectangle_4"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"

        android:layout_marginTop="-2dp"

        android:background="@drawable/rectangle_temu" />

    <ImageButton
        android:id="@+id/backbtn"
        android:layout_width="39dp"
        android:layout_height="38dp"
        android:layout_marginStart="4dp"

        android:layout_marginTop="13dp"
        android:background="@null"
        app:srcCompat="@drawable/chevron_left"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/Tem2u"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="44dp"
        android:text="Konsultasi"
        android:textColor="@color/grey"
        android:textSize="28sp"
        android:textStyle="bold" />
    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="35dp"
        android:layout_height="30dp"

        android:layout_alignParentStart="true"

        android:layout_alignParentTop="true"
        android:layout_marginStart="49dp"
        android:layout_marginTop="74dp"
        app:srcCompat="@drawable/lokasi_marker" />


    <TextView
        android:id="@+id/lokasi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"


        android:layout_alignParentTop="true"

        android:layout_marginStart="89dp"
        android:layout_marginTop="80dp"
        android:text="Lokasi Anda"
        android:textColor="@color/black"
        android:textSize="20sp"

        />

    <TextView
        android:id="@+id/yogya"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"


        android:layout_alignParentTop="true"

        android:layout_alignParentEnd="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="79dp"
        android:layout_marginEnd="52dp"
        android:layout_toEndOf="@+id/lokasi"
        android:text="Yogyakarta, DIY"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvSpesialisLists"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-2dp"
        android:layout_marginTop="207dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="5dp"
        tools:listitem="@layout/item_spesialis" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="167dp"
        android:text="Daftar Konsultasi Spesialis"
        android:textColor="@color/black"
        android:textSize="27sp"
        android:textStyle="bold" />


</RelativeLayout>




