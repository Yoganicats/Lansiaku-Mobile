<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FasilitasLayananActivity">


    <View
        android:id="@+id/rectangle_4"
        android:layout_width="wrap_content"
        android:layout_height="151dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"

        android:layout_marginTop="-3dp"
        android:layout_marginEnd="0dp"
        android:background="@drawable/rectangle_temu" />

    <ImageButton
        android:id="@+id/backbtn"
        android:layout_width="39dp"
        android:layout_height="38dp"
        android:layout_alignParentEnd="true"

        android:layout_marginStart="4dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="368dp"
        android:background="@null"
        app:srcCompat="@drawable/chevron_left"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/Tem2u"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="44dp"
        android:text="Temu Dokter"
        android:textColor="@color/grey"
        android:textSize="28sp"
        android:textStyle="bold" />


    <TextView
        android:id="@+id/lokasi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/rectangle_4"


        android:layout_alignParentStart="true"

        android:layout_alignParentTop="true"
        android:layout_marginStart="97dp"
        android:layout_marginTop="82dp"
        android:layout_marginBottom="39dp"
        android:text="Lokasi Anda"

        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/yogya"
        android:layout_width="146dp"
        android:layout_height="27dp"
        android:layout_alignBottom="@+id/rectangle_4"


        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="4dp"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="39dp"
        android:layout_toEndOf="@+id/lokasi"
        android:text="Yogyakarta, DIY"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="35dp"
        android:layout_height="30dp"

        android:layout_alignParentStart="true"

        android:layout_alignParentTop="true"
        android:layout_marginStart="63dp"
        android:layout_marginTop="79dp"
        app:srcCompat="@drawable/lokasi_marker" />


    <TextView
        android:id="@+id/Fasilit"
        android:layout_width="352dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"

        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"

        android:layout_centerHorizontal="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="198dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="36dp"
        android:gravity="center"
        android:text="Pilih Jenis Layanan"
        android:textColor="@color/black"
        android:textSize="27sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="358dp"

        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="149dp"
        android:layout_marginEnd="21dp"
        app:srcCompat="@drawable/step2_temudokter" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/RvKlinik"
        android:layout_width="408dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="241dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="32dp"
        tools:listitem="@layout/item_fasilitas_kesehatan" />


</RelativeLayout>




